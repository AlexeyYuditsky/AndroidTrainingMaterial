+        _state.value = State(signUpInProgress = true)
+    }
+
+    private fun goBack() {
+        _goBackEvent.value = Event(Unit)
+    }
+
+    private fun showSuccessSignUpMessage() {
+        _showSuccessSignUpMessageEvent.value = Event(Unit)
+    }
